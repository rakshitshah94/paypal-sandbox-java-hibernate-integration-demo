<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ShowProductServ</servlet-name>
        <servlet-class>ShowProductServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProceedToPayServ</servlet-name>
        <servlet-class>ProceedToPayServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderSaveServ</servlet-name>
        <servlet-class>OrderSaveServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveTransactionServ</servlet-name>
        <servlet-class>SaveTransactionServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowProductServ</servlet-name>
        <url-pattern>/ShowProductServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProceedToPayServ</servlet-name>
        <url-pattern>/ProceedToPayServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderSaveServ</servlet-name>
        <url-pattern>/OrderSaveServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveTransactionServ</servlet-name>
        <url-pattern>/SaveTransactionServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
